{

  "manifest_version": 2,
  "name": "Gamersclub Cards",
  "description": "Shows player cards on lobby and ranked open pages to get more statistics about then.",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "content_scripts": [
   {
      "matches": ["*://*.gamersclub.com.br/*"],
      "js": ["main.js"]
   }
 ]

}
